					else
						toggle1.setShadowLayer(0.0001, Math.round(toggle1.getLineHeight() / 8), Math.round(toggle.getLineHeight() / 8), android.graphics.Color.parseColor("#FF333333"));
					toggle1.setChecked(sdisp2);
